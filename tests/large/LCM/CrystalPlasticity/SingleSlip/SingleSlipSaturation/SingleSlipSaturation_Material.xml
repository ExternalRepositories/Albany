<ParameterList>
  <ParameterList name="ElementBlocks">

    <ParameterList name="element">
      <Parameter
          name="material"
          type="string"
          value="Rotated Lattice" />
    </ParameterList>

  </ParameterList>

  <ParameterList name="Materials">

    <ParameterList name="Rotated Lattice">

      <ParameterList name="Material Model">
        <Parameter
            name="Model Name"
            type="string"
            value="CrystalPlasticity" />
      </ParameterList>

      <!-- Crystal Elasticity moduli and orientation -->
      <ParameterList name="Crystal Elasticity">
        <Parameter
          name="C11"
          type="double"
          value="204.6e3" />
        <Parameter
          name="C12"
          type="double"
          value="137.7e3" />
        <Parameter
          name="C44"
          type="double"
          value="126.2e3" />
        <Parameter
            name="Basis Vector 1"
            type="Array(double)"
            value="{ -0.09175170953613698, 0.9082482904638630, 0.4082482904638630}" />
        <Parameter
            name="Basis Vector 2"
            type="Array(double)"
            value="{0.9082482904638630, -0.09175170953613698, 0.4082482904638630}" />
        <Parameter
            name="Basis Vector 3"
            type="Array(double)"
            value="{ 0.4082482904638630, 0.4082482904638630, -0.8164965809277260}" />
      </ParameterList>

      <Parameter
          name="Integration Scheme"
          type="string"
          value="Implicit" />
      <Parameter	  
          name="Implicit Integration Relative Tolerance"
          type="double"
          value="1.0e-45" />
      <Parameter
          name="Output Cauchy Stress"
          type="bool"
          value="true" />      
      <Parameter
          name="Implicit Integration Absolute Tolerance"
          type="double"
          value="1.0e-12" />
      <Parameter
          name="Implicit Integration Max Iterations Implicit"
          type="int"
          value="100" />
      <Parameter
          name="Residual Type"
          type="string"
          value="Slip Hardness" />
      <Parameter
          name="Number of Slip Systems"
          type="int"
          value="1" />      
      <Parameter
          name="Output CP_Residual"
          type="bool"
          value="true" />

      <ParameterList name="Slip System Family 0">

        <ParameterList name="Flow Rule">

          <Parameter
              name="Type"
              type="string"
              value="Power Law" />

          <Parameter
              name="Reference Slip Rate"
              type="double"
              value="1.0e-03" />

          <Parameter
              name="Rate Exponent"
              type="double"
              value="2.0e01" />

        </ParameterList>

        <ParameterList name="Hardening Law">

          <Parameter
              name="Type"
              type="string"
              value="Saturation" />
          <Parameter
              name="Hardening Rate"
              type="double"
              value="2.0e4" />
          <Parameter
              name="Initial Saturation Stress"
              type="double"
              value="202.0" />
          <Parameter
              name="Saturation Exponent"
              type="double"
              value="0.01" />
          <Parameter
              name="Reference Slip Rate"
              type="double"
              value="1.0e-03" />
          <Parameter
              name="Initial Hardening State"
              type="double"
              value="90.0" />
        </ParameterList>

      </ParameterList>

      <!-- Crystal Plasticity Slip System -->
      <ParameterList name="Slip System 1">
	<Parameter
            name="Slip Direction"
            type="Array(double)"
            value="{-1.0, 1.0, 0.0}" />
	<Parameter
            name="Slip Normal"
            type="Array(double)"
            value="{1.0, 1.0, 1.0}" />
      </ParameterList>      

    </ParameterList>

    <!-- Specify output to Exodus -->
    <Parameter
        name="Output Cauchy Stress"
        type="bool"
        value="true" />
    <Parameter
        name="Output Fp"
        type="bool"
        value="true" />
    <Parameter
        name="Output L"
        type="bool"
        value="true" />
    <Parameter
        name="Output gamma_1"
        type="bool"
        value="true" />
    <Parameter
        name="Output tau_hard_1"
        type="bool"
        value="true" />
    <Parameter
        name="Output tau_1"
        type="bool"
        value="true" />
    <Parameter
        name="Output CP_Residual"
        type="bool"
        value="true" />

  </ParameterList>

</ParameterList>
