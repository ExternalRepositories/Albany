
nem_slice -e -m mesh=1x2 -l multikl  -o pipe_med.nemI pipe_med.exo 
mkdir tmp1
echo "
Input FEM file		= pipe_med.exo
LB file			= pipe_med.nemI 
Debug			= 1
Restart Time list	= off
Reserve space		= nodal=1, elemental=0, global=0
Parallel Disk Info = number=1
Parallel file location = root=tmp,subdir=..
" > nem_spread.inp
nem_spread
rmdir tmp1
