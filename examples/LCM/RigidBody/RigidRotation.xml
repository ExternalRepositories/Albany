<!-- =========================================================================================== -->
<!-- Discretization and i/o information ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
<!-- =========================================================================================== -->

     <ParameterList name="Discretization">

          <Parameter name="Method" 
                     type="string" 
                     value="Exodus"/>

          <Parameter name="Exodus Input File Name" 
                     type="string" 
                     value="RigidRotation.g"/>

          <Parameter name="Exodus Output File Name" 
                     type="string" 
                     value="RigidRotation.exo"/>

          <Parameter name="Solution Vector Components"
                     type="Array(string)" 
                     value="{displacement, V}"/>

          <Parameter name="Residual Vector Components"
                     type="Array(string)"
                     value="{force, V}"/>

          <Parameter name="Cubature Degree" 
                     type="int" 
                     value="3"/>

          <Parameter name="Separate Evaluators by Element Block" 
                     type="bool" 
                     value="true"/>

     </ParameterList>
  


<!-- =========================================================================================== -->
<!-- Solver options ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
<!-- =========================================================================================== -->

     <ParameterList name="Piro">

          <Parameter name="Solver Type" 
                     type="string" 
                     value="LOCA"/>

          <ParameterList name="LOCA">

               <ParameterList name="Bifurcation"/>

                    <ParameterList name="Constraints"/>

                         <ParameterList name="Predictor">

                              <Parameter  name="Method" 
                                          type="string" 
                                          value="Constant"/>

                         </ParameterList>


<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
<!-- ~~~~~~~~~~~~~~~~~~~ PARAMETER STEPPING ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

                         <ParameterList name="Stepper">

                              <Parameter name="Continuation Method" 
                                         type="string" 
                                         value="Natural"/>

                              <Parameter name="Initial Value" 
                                         type="double" 
                                         value="0.0"/>

                              <Parameter name="Continuation Parameter" 
                                         type="string" 
                                         value="Time"/>

                              <Parameter name="Max Steps" 
                                         type="int" 
                                         value="12"/>

                              <Parameter name="Min Value" 
                                         type="double" 
                                         value="0.0"/>

                              <Parameter name="Max Value" 
                                         type="double" 
                                         value="12.0"/>

                              <Parameter name="Return Failed on Reaching Max Steps"
                                         type="bool" 
                                         value="0"/>

                              <Parameter name="Hit Continuation Bound" 
                                         type="bool" 
                                         value="0"/>

                         </ParameterList>

                          <ParameterList name="Step Size">

<!-- ~~~~~~~~~~~~~~~~~~~~~~~~ Control the parameter incrementation,
                              here it is the displacement increment on the BC ~~~~~~~~~~~~~~~~~~ -->

                              <Parameter name="Initial Step Size" 
                                         type="double" 
                                         value="1.0"/>

                              <Parameter name="Method" 
                                         type="string" 
                                         value="Constant"/>

                          </ParameterList>

          </ParameterList>



