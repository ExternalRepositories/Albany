    <!-- Right Dot Ohmic -->
    <ParameterList name="rdo_contact">
      <Parameter name="elementBlock" type="string" value="eb_nplussilicon" />
    </ParameterList>

    <!-- Left Charge Sensor Top Ohmic -->
    <ParameterList name="lcsto_contact">
      <Parameter name="elementBlock" type="string" value="eb_nplussilicon" />
    </ParameterList>

    <!-- Left Charge Sensor Bottom Ohmic -->
